{
  "name": "<%= name %>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:server": "webpack --watch --mode=development --config server/webpack.config.js",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "frourio --watch --config server/frourio.config.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "build": "npm run build:apis && npm run build:server",
    "build:server": "webpack --mode=production --config server/webpack.config.js",
    "build:apis": "cross-env NODE_ENV=production run-p build:aspida build:frourio",
    "build:aspida": "aspida --build",
    "build:frourio": "frourio --build --config server/frourio.config.js",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "npm run build:apis && tsc --noEmit",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@aspida/axios": "^0.6.0",
    "axios": "^0.19.2",
    "frourio": "^0.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.0",
    "nodemon-webpack-plugin": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^10.1.0",
    "ts-jest": "^25.0.0",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
